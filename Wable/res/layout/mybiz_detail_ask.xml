<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg"
    android:orientation="vertical" >
    
    <com.google.android.maps.MapView
        android:id="@+id/mapview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignBottom="@+id/TextView02"
        android:layout_below="@+id/relativeLayout2"
        android:layout_marginTop="-20dp"
        android:apiKey="0ifaiNwh-8lROoZpqpF9MxGODOf1i2mAo5RppHg"
        android:clickable="true"
        android:longClickable="true" >

    </com.google.android.maps.MapView>

    <RelativeLayout
        android:id="@+id/relativeLayout2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/textView9" >

        <RelativeLayout
            android:id="@+id/MYBIZPOST_REQtvToplayout"
            android:layout_width="wrap_content"
            android:layout_height="100dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:background="#000" >

            <TextView
                android:id="@+id/textView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:background="@drawable/service_info_providing_icon" />

            <TextView
                android:id="@+id/MYBIZPOST_REQtvTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="14dp"
                android:layout_toRightOf="@+id/textView1"
                android:text="TextView" />

            <TextView
                android:id="@+id/MYBIZPOST_REQtvPrice"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/textView1"
                android:layout_alignLeft="@+id/MYBIZPOST_REQtvTitle"
                android:text="50,000" />

            <TextView
                android:id="@+id/MYBIZPOST_REQtvOther"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_above="@+id/MYBIZPOST_REQtvPrice"
                android:layout_alignParentRight="true"
                android:layout_marginRight="5dp"
                android:background="@drawable/service_info_timebar_red" />

            <TextView
                android:id="@+id/TextView03"
                android:layout_width="wrap_content"
                android:layout_height="1dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/MYBIZPOST_REQtvPrice"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/asking_contents_dotline_red" />

            <TextView
                android:id="@+id/MYBIZPOST_REQtvContents"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/TextView03"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="5dp"
                android:text="14124444444444444444444444444v12v214v124v12v124v112v124124v124v124v12412 sd hfks hkw w fw ehw3 fohw fwh 3 hwkj hwk3 hka hk " />

        </RelativeLayout>

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/MYBIZPOST_REQtvToplayout"
            android:layout_alignParentRight="true"
            android:layout_marginBottom="-20dp"
            android:background="@drawable/userinfo_down_btn_red" />

    </RelativeLayout>

    <ListView
        android:id="@+id/listView1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignTop="@+id/textView8"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="8dp" >
    </ListView>

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="8dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="158dp"
        android:background="@drawable/asking_contents_sub_menu_redbar" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/top_indicator_red"
        android:gravity="center"
        android:text="제공 내용"
        android:textColor="#FFF"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/TextView02"
        android:layout_width="60dp"
        android:layout_height="25dp"
        android:layout_alignBaseline="@+id/TextView01"
        android:layout_alignBottom="@+id/TextView01"
        android:layout_alignParentRight="true"
        android:layout_marginRight="20dp"
        android:background="@drawable/userinfo_sub_menu_basic_btn" />

    <TextView
        android:id="@+id/TextView01"
        android:layout_width="60dp"
        android:layout_height="25dp"
        android:layout_above="@+id/listView1"
        android:layout_toLeftOf="@+id/TextView02"
        android:background="@drawable/asking_contetns_sub_menu_selected_red_btn" />

</RelativeLayout>