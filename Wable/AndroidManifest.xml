<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wable"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="8" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_MOCK_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
         <uses-library android:name="com.google.android.maps" /> 
        
        
        <activity
            android:name=".WableActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".login.PasswordFindActivity" android:launchMode="singleTop"></activity>
        <activity android:name=".login.RegisterActivity" android:launchMode="singleTop"></activity>
        <activity android:name=".mypage.RequestListActivity"></activity>
        <activity android:name=".mypage.MypageActivity" android:launchMode="singleTop"></activity>
        <activity android:name=".mypage.OfferListActivity"></activity>
        <activity android:name=".main.MainActivity"></activity>
        <activity android:name=".post.RequestCategory"></activity>
        <activity android:name=".post.PostActivity" android:launchMode="singleTop"></activity>
        <activity android:name=".post.RequestPost"></activity>
        <activity android:name=".post.RequestPostList"></activity>
        <activity android:name=".post.RequestPostSubmit"></activity>
        <activity android:name=".post.RequestCategoryDetail"></activity>
        <activity android:name=".search.SearchActivity"></activity>
        <activity android:name=".setting.SettingActivity"></activity>
    </application>

</manifest>